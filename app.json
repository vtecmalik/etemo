{
  "expo": {
    "name": "Etemo",
    "slug": "etemo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5F8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.etemo.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Etemo использует камеру для сканирования штрих-кодов косметики"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F5F5F8"
      },
      "package": "com.etemo.app",
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Разрешите доступ к камере для сканирования штрих-кодов"
        }
      ],
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "enforceNavigationBarContrast": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c0743a36-1156-4bec-85d2-a1fec9695067"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c0743a36-1156-4bec-85d2-a1fec9695067"
    }
  }
}
